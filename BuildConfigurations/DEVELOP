# Default configuration, used if none other is specified at configure time via
# EXEC_BUILD_CONFIG=xxx or --with-build-config=xxx (the latter overidding the
# former)

ac_enable debug
ac_enable warnings-as-errors
ac_disable silent-rules
ac_enable monolith
ac_enable boot-progress
ac_enable pal-diagnostics
ac_enable self-tests

